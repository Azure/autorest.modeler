{
  "openapi": "3.0.0",
  "servers": [],
  "info": {
    "version": "1.0.9-abcd",
    "title": "Swagger Sample API"
  },
  "paths": {
    "/pets/": {
      "get": {
        "responses": {
          "200": {
            "description": "pet response"
          }
        }
      }
    }
  },
  "components": {
    "securitySchemes": {
      "petstore_auth": {
        "type": "oauth2",
        "flows": {
          "implicit": {
            "authorizationUrl": "http://petstore.swagger.io/api/oauth/dialog",
            "scopes": {
              "write:pets": "modify pets in your account",
              "read:pets": "read your pets"
            }
          }
        }
      },
      "api_key": {
        "type": "apiKey",
        "name": "api_key",
        "in": "header"
      }
    }
  }
}